{
  "name": "tlp-recorder",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/src/index.js",
  "bin": "./dist/src/index.js",
  "pkg": {
    "assets": [
      "node_modules/.prisma/**/*"
    ],
    "scripts": [
      "./dist/**/*.js"
    ],
    "targets": [
      "node16-win-x64"
    ],
    "outputPath": "pkg"
