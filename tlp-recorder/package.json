    "prettier": "^2.6.2",
    "prisma": "^3.14.0",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.6.7",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "google-auth-library": "^8.0.2",
    "googleapis": "^100.0.0",
    "memoryjs": "^3.4.0",
    "npmlog": "^6.0.2",
    "obs-websocket-js": "^5.0.0-beta.1",
    "open": "^8.4.0",
