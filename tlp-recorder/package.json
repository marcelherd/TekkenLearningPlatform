  "scripts": {
    "test": "jest",
    "start": "ts-node -r tsconfig-paths/register --files ./src/index.ts",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --respawn --rs --transpile-only --files ./src/index.ts",
    "build": "tsc && tsc-alias"
  },
  "author": "Marcel Herd",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
